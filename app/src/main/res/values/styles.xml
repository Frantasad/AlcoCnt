<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="itemClickable" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:background">@drawable/event_item_style</item>
    </style>

    <style name="itemNonClickable" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:background">@drawable/drink_item_style</item>
    </style>


</resources>
